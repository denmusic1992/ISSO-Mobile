<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ISSO_S.Testik">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" x:Name="Stack">
            <BoxView VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" BackgroundColor="Blue" x:Name="BlueBox"/>
            <BoxView VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" BackgroundColor="Green" x:Name="GreenBox"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>